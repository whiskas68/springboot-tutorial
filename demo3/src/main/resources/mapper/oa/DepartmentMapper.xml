<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.whiskas68.booter.mapper.oa.DepartmentMapper">
    <select id="selectAll" resultType="com.whiskas68.booter.entity.Department">
        select * from department;
    </select>

    <select id="selectBySn" resultType="com.whiskas68.booter.entity.Department">
        select * from department where sn=#{sn}
    </select>
</mapper>